+                #address-cells = <1>;
+                #size-cells = <1>;
+                ranges = <0 0x10050000 0x10000>;
+
+                dma_pool: dma_pool@0 {
+                        reg = <0x0 0x10000>;
+                        pool;
+                };
+        };
+};
